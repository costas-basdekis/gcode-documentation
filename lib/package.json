  "keywords": [
    "gcode",
    "3d-printing",
    "marlin",
    "reprap",
    "klipper3d"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "mocha": "^9.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
